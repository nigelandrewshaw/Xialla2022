        "c8bd3890-128a-4236-a845-c3887773c6b4": {
          "type": "collapsible_tab",
          "settings": {
            "heading": "Instructions",
            "icon": "lightning_bolt",
            "content": "<p>Xialla can be put on in a few seconds, so it's ready when you are. <\/p><p>Full instructions: <a href=\"\/pages\/instructions\" title=\"\/pages\/instructions\">Click here<\/a>.<\/p>",
            "page": ""
